{
  "name": "mongo-schematic-class",
  "version": "1.0.2",
  "description": "Mongoose Schema builder for ES6 classes that supports multilevel inheritance.",
  "main": "index.js",
  "scripts": {
    "build": "",
    "test": "npm run build && nyc mocha --recursive './test/**/*.spec.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Omninox/SchematicClass.git"
  },
  "keywords": [
    "mongoose",
    "mongodb",
    "mongo",
    "schema",
    "class",
    "es6",
    "multilevel",
    "inheritance"
  ],
  "author": "Calvin Moore (c1moore@outlook.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Omninox/SchematicClass/issues"
  },
  "homepage": "https://github.com/Omninox/SchematicClass#readme",
  "devDependencies": {
    "@types/mongoose": "^5.3.26",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "should": "^13.2.3",
    "sinon": "^7.5.0"
  },
  "dependencies": {
    "mongoose": ">=4 <6"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "exclude": [
      "test/**",
      "**/*.d.ts",
      "coverage/**"
    ],
    "reporter": [
      "json",
      "lcov",
      "text"
    ],
    "statements": 80,
    "branches": 80
  },
  "mocha": {
    "recursive": true,
    "color": true,
    "exit": true
  }
}
